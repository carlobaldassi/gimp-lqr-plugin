msgstr "Camada selecionada"

#. New size
#: ../src/interface.c:309
msgid "Select new width and height"
msgstr "Selecione nova largura e altura"

#: ../src/interface.c:329
msgid "Width:"
msgstr "Largura: "

#: ../src/interface.c:331
msgid "Height:"
msgstr "Altura:"

